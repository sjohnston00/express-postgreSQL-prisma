generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Role {
  id    Int    @id @default(autoincrement())
  name  String
  users User?
}

model User {
  id        Int    @id @default(autoincrement())
  firstname String
  lastname  String
  age       Int
  email     String @unique @db.VarChar(255)
  role      Role   @relation(fields: [id], references: [id])
}
