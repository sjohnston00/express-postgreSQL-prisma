{
  "name": "express-postgresql",
  "version": "1.0.0",
  "description": "CRUD REST API using postgreSQL database",
  "scripts": {
    "start": "npm run compile:ts && node dist/index.js",
    "compile:ts": "tsc",
    "dev:js": "nodemon dist/index.js",
    "dev:ts": "tsc -w",
    "dev": "concurrently \"npm run dev:ts\" \"npm run dev:js\""
  },
  "author": "Sam Johnston",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.8.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
